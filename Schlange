#include <iostream>
using namespace std;
int main() {
    int a;
    cout << "Geben Sie ein Nummer: ";
    cin >> a;
    for (int i = 0; i < a; i+=2) { 
        cout << "*";
        if (i != a-1)
            for (int j = 1; j < 2; j+=2) { 
                cout << "#";
            }
        else
            break;
    }
    return 0;
}
